  "bugs": {
    "url": "https://github.com/SoYoung210/design-system-config/issues"
  },
  "homepage": "https://github.com/SoYoung210/design-system-config#readme",
  "browserslist": [
    "defaults",
    "> 5%",
    "not ie > 0",
    "ios_saf >= 11",
    "not dead"
  ],
  "peerDependencies": {
    "react": "^17.0.1",
